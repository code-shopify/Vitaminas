.bg-pri{background-color:#fffcf7 !important;}


/*---------------- Global Custom CSS -------------------*/
{{ settings.global_css }}
button.nav-button.prev-button:hover {
    color: black;
}
button.nav-button.next-button:hover {
    color: black;
}



